{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["react", "next", "airbnb-base", "prettier"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint", "prettier", "import"],
	"rules": {},
	"settings": {
		"import/resolver": {
			"typescript": {
				"project": [
					"./tsconfig.json"
				  ]
			}
		}
	}
}
